    <div class="page-container" id="top">
      <div class="page-content">
        <header class="jumbotronHeader" id="jumbotronHeader">
          <picture>
            <source
              type="image/webp"
              sizes="100vw"
              srcset="img/Ingo-Steinke-flower-shirt.webp 1x,
                    img/Ingo-Steinke-flower-shirt-496.webp 2x,
                    img/Ingo-Steinke-flower-shirt-large.webp 4x"
            >
            <img
              src="img/Ingo-Steinke-flower-shirt.jpg"
              srcset="img/Ingo-Steinke-flower-shirt-496.jpg 2x,
                    img/Ingo-Steinke-flower-shirt-large.jpg 4x"
              width="248"
              height="248"
              alt="{{ content.jumbotronImageAlt }}"
              class="avatar"
            >
          </picture>
          <h1>{{ content.jumbotronHeader }}</h1>
          <p class="tagline">{{ content.jumbotronTagline }}</p>
        </header>
        <div class="sticky-header is-nav-closed" id="sticky-header">
          <div class="wrapper">
            <span class="raised-layer">
              <span class="scripted hideable" id="sticky-headline">
                <a href="#top">
                  <img
                    src="img/Ingo-Steinke-portrait-2020.jpg"
                    width="25"
                    height="25"
                    alt="{{ content.linkToTop }}"
                    class="avatar"
                  />
                  {{ content.jumbotronHeader }}
                </a>
              </span>
            </span>

            <div class="nav-wrapper">
              <a href="#contact" aria-hidden="true" class="extra-nav-link initially-hidden">{{ content.targetContact }}</a>
              <a href="#main-menu"
                id="main-menu-toggle"
                class="menu-toggle"
                aria-label="{{ content.menuOpen }}">
                <span class="sr-only">{{ content.menuOpen }}</span>
                {{ content.menuButton }}
              </a>

              <nav
                id="main-menu"
                class="main-menu page-navigation"
                aria-label="{{ content.menuLabel }}"
              >
                <a
                  href="#main-menu-toggle"
                  id="main-menuClose"
                  class="menuClose"
                  aria-label="{{ content.menuClose }}"
                >
                  <svg viewBox="0 0 512 512" width="16" height="16">
                    <g>
                      <path d="m25 512a25 25 0 0 1 -17.68-42.68l462-462a25 25 0 0 1 35.36 35.36l-462 462a24.93 24.93 0 0 1 -17.68 7.32z" fill="currentColor"/>
                      <path d="m487 512a24.93 24.93 0 0 1 -17.68-7.32l-462-462a25 25 0 0 1 35.36-35.36l462 462a25 25 0 0 1 -17.68 42.68z" fill="currentColor"/>
                    </g>
                  </svg>
                </a>
                <ul>
                  <li><a href="#projects">{{ content.targetProjects }}</a></li>
                  <li><a href="#skills">{{ content.targetSkills }}</a></li>
                  <li><a href="#about-me">{{ content.targetAboutMe }}</a></li>
                  <li><a href="#contact">{{ content.targetContact }}</a></li>
                  <li><a href="#imprint">{{ content.targetImprint }}</a></li>
                  <li><a href="#privacy">{{ content.targetPrivacy }}</a></li>
                  <li>
                    <a
                      href="{{ content.alternateHref }}"
                      hreflang="{{ content.alternateHreflang }}"
                      lang="{{ content.alternateHreflang }}"
                      title="{{ content.targetOtherLanguageTitle }}"
                      id="language-switch"
                    >{{ content.targetOtherLanguage }}</a>
                  </li>
                </ul>
              </nav>

              <a href="#main-menu-toggle"
                class="backdrop"
                tabindex="-1"
                aria-hidden="true"
                hidden>
              </a>
