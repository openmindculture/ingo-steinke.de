                  <a href="https://dev.to/ingosteinke"
                    target="_blank"
                    rel="noopener"
                    title="dev.to-{{ content.profile }}"
                  >
                    <svg width="30" height="25" viewBox="0 0 50 40" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <title>dev.to Logo</title>
                      <rect width="50" height="40" rx="3" style="fill:none;stroke:currentColor;stroke-width:4px;"></rect>
