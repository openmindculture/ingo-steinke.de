            </article>

            <aside id="privacy" class="privacy initially-hidden clear target-offset text-content-color">
              <div class="aside-heading">{{ content.privacyHeading }}</div>
              <p>
                {{ content.privacyGdpr }} <a href="#imprint">{{ content.privacyGdprImprint }}</a> {{ content.privacyGdpr2 }}
              </p>
              <div class="aside-heading">{{ content.privacyYourRightsHeading }}</div>
              <p>{{ content.privacyYourRights }}</p>
              <div class="aside-heading">{{ content.privacyPersonalDataHeading }}</div>
              <p>{{ content.privacyPersonalData }}</p>
              <div class="aside-heading">{{ content.privacyLawHeading }}</div>
              <p>{{ content.privacyLaw }}</p>
              <p>{{ content.privacyLaw2 }}</p>
              <div class="aside-heading">{{ content.privacyMinimalHeading }}</div>
              <p>{{ content.privacyMinimal }}</p>
              <p>{{ content.privacyNoCookies }}</p>
              <p>{{ content.privacyLinkDisclaimer }}</p>
              <div class="aside-heading">{{ content.privacyLogfilesHeading }}</div>
              <p>{{ content.privacyLogfiles }}</p>
              <p>{{ content.privacyLogfilesLaw }}</p>
              <div class="aside-heading">{{ content.privacyChangesHeading }}</div>
              <p>{{ content.privacyChanges }}</p>
            </aside>
          </section>
        </main>

        <footer class="page-footer">

          <p class="legal legal--links no-print">
            <a href="#contact">{{ content.targetContact }}</a>
            <a href="#imprint">{{ content.targetImprint }}</a>
            <a href="#privacy">{{ content.targetPrivacy }}</a>
          </p>

          {% comment %} hide untranslated content using content variable{% endcomment %}
          {% if content.showLandingpageLinks == "true" %}
          <p class="landingpage--links no-print">
            <a href="{{ content.landingLinkUrlWordPress }}" title="{{ content.landingLinkTitleWordPress }}">{{ content.landingLinkTextWordPress }}</a>
            <a href="{{ content.landingLinkUrlShopware }}" title="{{ content.landingLinkTitleShopware }}">{{ content.landingLinkTextShopware }}</a>
            <a href="{{ content.landingLinkUrlSustainability }}" title="{{ content.landingLinkTitleSustainability }}">{{ content.landingLinkTextSustainability }}</a>
          </p>
          {% endif %}

          <div id="wcb" class="carbonbadge no-print"></div>
          <script src="https://unpkg.com/website-carbon-badges@1.1.3/b.min.js" defer></script>

          <div class="center transparent no-print">
            <small>{{ content.listedAt }} <a href="http://www.suchnadel.de" title="suchnadel.de">Suchnadel</a></small>
          </div>

          <div class="center transparent no-print">
            <p>
              <a
                href="{{ content.ecofriendlyEcograderUrl }}"
                target="_blank"
                rel="noopener"
                title="{{ content.ecofriendlyEcograderTitle }}"
              >
                {{ content.ecofriendlyEcograderScore }}
              </a>
            </p>
          </div>

          <div class="badges">
            <div class="badge badge--greenweb transparent no-print">
              <a
                href="{{ content.ecofriendlyGreenWebCheckUrl }}"
                target="_blank"
                rel="noopener"
              >
                <picture>
                  <source srcset="img/{{ content.ecofriendlyGreenWebCheckImageFilename }}.webp" type="image/webp">
                  <source srcset="img/{{ content.ecofriendlyGreenWebCheckImageFilename }}.png" type="image/png">
                  <img
                     src="img/{{ content.ecofriendlyGreenWebCheckImageFilename }}.png"
                     data-original-src="{{ content.ecofriendlyGreenWebCheckImageOriginalSrc }}"
                     alt="{{ content.ecofriendlyGreenWebCheckImageAlt }}"
                  >
                </picture>
              </a>
            </div>
          </div>

          <p class="no-print">
            <button id="animation-toggle">{{ content.buttonStopAnimation }}</button>
          </p>

          <div class="only-print"><br></div>

          <div class="legal legal--copyright">
            &copy; {{ "now" | date: "%Y" }} {{ content.imprintName }}
            ({{ content.updatedWithColon }}
            {{ "now" | date: "%d" }}{{ content.dotIfGerman }}
            {{ "now" | date: "%m" | customLocalizedMonthNameFilter }}
            {{ "now" | date: "%Y" }})
          </div>

        </footer>
      </div>

      <aside class="decoration" id="decoration">
        <div class="animation-wrapper">
          <div class="flounder flounder--pie position position--bottom-left"></div>
          <div class="flounder flounder--pie position position--top-right"></div>
          <div class="flounder flounder--pie position position--bottom-right"></div>
        </div>
      </aside>
    </div>
  </body>
</html>
