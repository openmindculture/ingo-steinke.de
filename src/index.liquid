<!doctype html>
<html lang="{{ '' | appendCurrentLanguageSlug }}">
  <head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <title>
      {{ content.pageTitle }}
    </title>
    <meta name="language" content="{{ '' | appendCurrentLanguageSlug }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
      /* heading font serif */
      @font-face {
        font-family: 'Bitter';
        font-style: normal;
        font-weight: 700;
        font-display: swap;
        src:
          local(''),
          url('fonts/bitter-v16-latin-700.woff2') format('woff2'),
          url('fonts/bitter-v16-latin-700.woff') format('woff');
      }

      /* body font sans */
      @font-face {
        font-family: 'Montserrat';
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src:
          local(''),
          url('fonts/montserrat-v15-latin-ext_latin-regular.woff2') format('woff2'),
          url('fonts/montserrat-v15-latin-ext_latin-regular.woff') format('woff');
      }
      {%- postcss -%}
        {% include './_includes/critical.css' %}
      {%- endpostcss -%}
    </style>
    <link rel="preload" href="fonts/bitter-v16-latin-700.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="fonts/montserrat-v15-latin-ext_latin-regular.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="stylesheet" media="screen" href="styles.css?v=4.4.5">
    <script defer src="js/scripts.js?v=4.4.5"></script>
    {% comment %}
    Defer non-critical CSS as suggested at https://web.dev/articles/defer-non-critical-css
    {% endcomment %}
    <link
      rel="preload" as="style"
      onload="this.onload=null;this.rel='stylesheet'"
      media="screen and (prefers-reduced-motion: no-preference)"
      href="decoration.css?v=4.4.5"
    >
    <noscript>
      <link rel="stylesheet" media="screen and (prefers-reduced-motion: no-preference)" href="decoration.css?v=4.4.5">
    </noscript>
    <link rel="stylesheet" media="print" href="print.css?v=4.4.5">
    <meta name=author content="{{ content.metaAuthor }}">
    <meta
      property="og:title"
      content="{{ content.pageTitle }}"
    >
    <meta
      property="og:description"
      content="{{ content.pageDescription }}"
    >
    <meta name=description content="{{ content.pageDescription }}">
    <meta name="p:domain_verify" content="{{ content.pDomainVerify }}"/>
    <meta name="google-site-verification" content="{{ content.googleSiteVerification }}"/>
    <meta name=robots content="index,follow">
    <meta
      property="og:image"
      content="{{ content.canonicalHref }}img/Ingo-Steinke-portrait-2020.jpg"
    >
    <meta
      property="og:url"
      content="{{ content.canonicalHref }}"
    >
    <link
      rel="canonical"
      href="{{ content.canonicalHref }}"
    >
    <link
      rel="alternate"
      hreflang="{{ content.alternateHreflang }}"
      href="{{ content.alternateHref }}"
    >
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
    <script type="application/ld+json">
      {
        "@context": "https://schema.org/",
        "@type": "AggregateRating",
        "itemReviewed": {
          "@type": "LocalBusiness",
          "name": "Ingo Steinke",
          "address" :{
            "@type": "PostalAddress",
            "streetAddress": "Donaustr. 83 II",
            "addressLocality": "Berlin",
            "addressRegion": "Berlin",
            "postalCode": "12043",
            "addressCountry": "DE"
          }
        },
        "ratingValue": "5",
        "bestRating": "5",
        "ratingCount": "7"
      }
    </script>
  </head>
  <body class="high-contrast">
    <div class="page-container" id="top">
      <div class="page-content">
        <header class="jumbotronHeader" id="jumbotronHeader">
          {% include './_includes/jumbotron-image.liquid' %}
          <h1>{{ content.jumbotronHeader }}</h1>
          <p class="tagline">{{ content.jumbotronTagline }}</p>
        </header>
        <div class="sticky-header is-nav-closed is-unstuck" id="sticky-header">
          <div class="wrapper">
            <span class="raised-layer">
              <span class="scripted hideable" id="sticky-headline">
                <a href="#top">
                  <img
                    src="img/Ingo-Steinke-portrait-2020.jpg"
                    width="25"
                    height="25"
                    alt="{{ content.linkToTop }}"
                    class="avatar"
                  />
                  {{ content.jumbotronHeader }}
                </a>
              </span>
            </span>

            <div class="nav-wrapper">
              <a href="#contact" class="extra-nav-link initially-hidden">{{ content.targetContact }}</a>
              <a href="#main-menu"
                id="main-menu-toggle"
                class="menu-toggle"
                aria-label="{{ content.menuOpen }}">
                <span class="sr-only">{{ content.menuOpen }}</span>
                {{ content.menuButton }}
              </a>

              <nav
                id="main-menu"
                class="main-menu page-navigation"
                aria-label="{{ content.menuLabel }}"
              >
                <a
                  href="#main-menu-toggle"
                  id="main-menuClose"
                  class="menuClose"
                  aria-label="{{ content.menuClose }}"
                >
                  <svg viewBox="0 0 512 512" width="16" height="16">
                    <g>
                      <path d="m25 512a25 25 0 0 1 -17.68-42.68l462-462a25 25 0 0 1 35.36 35.36l-462 462a24.93 24.93 0 0 1 -17.68 7.32z" fill="currentColor"/>
                      <path d="m487 512a24.93 24.93 0 0 1 -17.68-7.32l-462-462a25 25 0 0 1 35.36-35.36l462 462a25 25 0 0 1 -17.68 42.68z" fill="currentColor"/>
                    </g>
                  </svg>
                </a>
                <ul>
                  <li><a href="#projects">{{ content.targetRefs }}</a></li>
                  <li><a href="#services">{{ content.targetServices }}</a></li>
                  <li><a href="{{ content.targetAboutMeUrl }}#about-me">{{ content.targetAboutMe }}</a></li>
                  <li><a href="#contact">{{ content.targetContact }}</a></li>
                  <li><a href="#imprint">{{ content.targetImprint }}</a></li>
                  <li><a href="#privacy">{{ content.targetPrivacy }}</a></li>
                  <li>
                    <a
                      href="{{ content.alternateHref }}"
                      hreflang="{{ content.alternateHreflang }}"
                      lang="{{ content.alternateHreflang }}"
                      title="{{ content.targetOtherLanguageTitle }}"
                      id="language-switch"
                    >{{ content.targetOtherLanguage }}</a>
                  </li>
                </ul>
              </nav>
              {% include '_includes/animation-toggle-button.liquid' %}
              <button
                id="contrast-toggle"
                class="contrast-toggle no-print"
                title="{{ content.toggleContrast }}"
                data-reducecaption="{{ content.reduceContrast }}"
                data-increasecaption="{{ content.increaseContrast }}"
              >
                <svg viewBox="0 0 32 32" width="24" height="24">
                  <path fill="currentColor" d="M15 2h2v3h-2zM27 15h3v2h-3zM15 27h2v3h-2zM2 15h3v2H2zM5.45 6.884l1.414-1.415 2.121 2.122-1.414 1.414zM23 7.58l2.121-2.12 1.414 1.414-2.121 2.121zM23.002 24.416l1.415-1.414 2.12 2.122-1.413 1.414zM5.47 25.13 7.59 23 9 24.42l-2.12 2.12-1.41-1.41zM16 8a8 8 0 1 0 8 8 8 8 0 0 0-8-8Zm0 14a6 6 0 0 1 0-12Z"/>
                  <path fill="none" style="fill:none" d="M0 0h32v32H0z"/>
                </svg>
              </button>

              <a href="#main-menu-toggle"
                class="backdrop"
                tabindex="-1"
                aria-hidden="true"
                hidden>
              </a>
            </div>

            </div>

        </div>

        <main id="content-top" class="target-offset">
          <section class="text-content-layout text-content-color" id="about-me">
            <article class="text-width headingless">
              <p>
                {{ content.intro }}
              </p>

              <h3>{{ content.workWithMeHeading }}</h3>

              <p>
                {{ content.workWithMe }}<br>
                {{ content.workWithMe2 }}<br>
                <a href="#contact">{{ content.ctaContact }}</a>
              </p>

              <h3 id="pricing" class="target-offset">{{ content.pricingHeading }}</h3>

              <p>
                {{ content.pricing }}
              </p>

            </article>

            <h3 id="testimonials">{{ content.testimonials }}</h3>
            <div class="testimonials">
              <figure class="testimonial">
                <figcaption>
                  {{ content.testimonialDanielaAuthor }}:
                  <span class="stars" title="{{ content.testimonialsFiveStarRating }}"></span>
                </figcaption>
                <blockquote>{{ content.testimonialDanielaQuote }}</blockquote>
              </figure>
              <figure class="testimonial">
                <figcaption>
                  {{ content.testimonialMalteAuthor }}:
                  <span class="stars" title="{{ content.testimonialsFiveStarRating }}"></span>
                </figcaption>
                <blockquote>{{ content.testimonialMalteQuote }}</blockquote>
              </figure>
              <figure class="testimonial">
                <figcaption>
                  {{ content.testimonialPatriciaAuthor }}:
                  <span class="stars" title="{{ content.testimonialsFiveStarRating }}"></span>
                </figcaption>
                <blockquote>{{ content.testimonialPatriciaQuote }}</blockquote>
              </figure>
            </div>

            <a href="{{ content.testimonialsReadMoreLinkUrl1 }}" target="_blank" rel="noopener">
              {{ content.testimonialsReadMoreText1 }}
            </a>
          </section>

          <section class="tiled-content target-offset" id="projects">
            <div class="pl1">
              <h2>{{ content.projectsHeading }}</h2>
              <p>
                {{ content.projectsHereAreSome }}
                </p>
            </div>

            <div class="grid-container grid-container--projects flip-box-container">
              {% include './_includes/projects.liquid',
                projectlist: 'kleiderordnung, shopwareplugin, twistedconnection',
                showflipsides: 'showflipsides'
              %}
              {% include './_includes/projects.liquid',
                projectlist: 'portfolio, daskannstdutun, fractal',
                showicons: 'showicons'
              %}
            </div>

            <br>

            <div class="pl1 transparent">
              {{ content.projectsMore1 }} <a href="{{ content.targetAboutMeUrl }}#projects">
                {{ content.projectsMoreLinkText }}
              </a>.
            </div>

          </section>

          <section class="text-content-layout text-content-color avoid-page-break">
            <h2 class="target-offset" id="work">
              {{ content.workHeading }}
            </h2>
            <article class="text-width">
              <p>
                {{ content.work }}
              </p>

              <h3>{{ content.workReliableHeading }}</h3>
              <p>
                {{ content.workReliable }}
                {{ content.workClientsBenefit }}
              </p>

              <h3 class="target-offset" id="zertifizierter-shopware-entwickler">{{ content.workCertifiedHeading }}</h3>
              <p>
                {{ content.workCertifiedIam }}
                <a href="{{ content.landingLinkUrlShopware }}">{{ content.workCertifiedSWDev }}</a>
                {{ content.workCertifiedAnd }}
                <a href="{{ content.workCertifiedSWDevPartnerPageUrl }}"
                   target="_blank"
                   rel="noopener"
                >{{ content.workCertifiedExtensionPartner }}</a>.
                <a
                  href="https://github.com/openmindculture"
                  hreflang="en"
                  target="_blank"
                  rel="me noopener"
                >{{ content.workCertifiedSustainableSourceCodeLinkText }}</a>
                {{ content.workCertifiedSustainable2 }}
                <a href="#website-carbon-certified">
                  {{ content.workCertifiedSustainableFootprintLinkText }}
                </a>
                {{ content.workCertifiedSustainable3 }}
              </p>

              <h3 id="interested">{{ content.interestedHeading }}</h3>
              <p>
                {{ content.interested }}
              </p>
            </article>
          </section>

          <section class="no-print text-content-layout services-wrapper avoid-page-break">
            <h2 id="services">{{ content.services }}</h2>

            <ul class="grid-container services pl0">
              <li class="tile text-content-color">
                <h4 class="font-sans">{{ content.servicesBuildTitle }}</h4>
                <ul>
                  <li>{{ content.servicesBuild1 }}</li>
                  <li>{{ content.servicesBuild2 }}</li>
                  <li>{{ content.servicesBuild3 }}</li>
                  <li>{{ content.servicesBuild4 }}</li>
                </ul>
                <a href="{{ content.servicesBuildMoreLinkPath }}" class="buttonlike" title="{{ content.servicesBuildMoreLinkTitle }}">{{ content.servicesBuildMoreLinkText }}!</a>
              </li>
              <li class="tile text-content-color">
                <h4 class="font-sans">{{ content.servicesVerifyTitle }}</h4>
                <ul>
                  <li>{{ content.servicesVerify1 }}</li>
                  <li>{{ content.servicesVerify2 }}</li>
                  <li>{{ content.servicesVerify3 }}</li>
                  <li>{{ content.servicesVerify4 }}</li>
                </ul>
                <a class="buttonlike" href="{{ content.servicesVerifyMoreLinkPath }}" title="{{ content.servicesVerifyMoreLinkTitle }}">{{ content.servicesVerifyMoreLinkText }}!</a>
              </li>
              <li class="tile text-content-color">
                <h4 class="font-sans">{{ content.servicesImproveTitle }}</h4>
                <ul>
                  <li>{{ content.servicesImprove1 }}</li>
                  <li>{{ content.servicesImprove2 }}</li>
                  <li>{{ content.servicesImprove3 }}</li>
                  <li>{{ content.servicesImprove4 }}</li>
                </ul>
                <a class="buttonlike" href="{{ content.servicesImproveMoreLinkPath }}" title="{{ content.servicesImproveMoreLinkTitle }}">{{ content.servicesImproveMoreLinkText }}!</a>
              </li>
            </ul>
          </section>

          <section class="imprint-contact">
            <div class="aside-heading center target-offset" id="imprint">
              {{ content.imprintHeader }}
            </div>
            <article id="contact" class="target-offset">
              <div class="vcard">
                <h2 class="fn">{{ content.imprintName }}</h2>
                <br>
                <span class="adr">
                  <span class="street-address">
                    {{ content.imprintStreetAddress }}</span><br>
                  <span class="postal-code">{{ content.imprintPostalCode }}</span> {{ content.imprintCity }}<br>
                  <span class="country-name">{{ content.imprintCountryName }}</span><br>
                </span><br>

                <span><a href="mailto:{{ content.contactEmail }}">{{ content.contactEmail }}</a></span><br>
                {% if content.contactInterimTel %}
                  <small>{{ content.contactTelOfflineLabel }}:<br></small>
                  <del>
                    <span class="tel">
                      <a href="tel:{{ content.contactTel }}">{%-
                          for chunk in content.contactTelChunks -%}<span class="visually-spaced">{{ chunk }}</span>
                        {%- endfor -%}</a>
                    </span><br>
                  </del>
                  <small>{{ content.contactInterimLabel }}:<br></small>
                  <span class="tel">
                      <a href="tel:{{ content.contactInterimTel }}">{%-
                          for chunk in content.contactInterimTelChunks -%}<span class="visually-spaced">{{ chunk }}</span>
                        {%- endfor -%}</a>
                    </span><br><br>
                {% else %}
                  <span class="tel">
                      <a href="tel:{{ content.contactTel }}">{%-
                          for chunk in content.contactTelChunks -%}<span class="visually-spaced">{{ chunk }}</span>
                        {%- endfor -%}</a>
                    </span><br>
                {% endif %}
                <div class="visually-hidden no-print">{{ content.contactTel }}</div>

                <br>
                <!-- contact form wrapper must be inside #contact anchor target for graceful show+hide -->
                <div class="contactform-wrapper initally-hidden no-print">
                  {% include './_includes/contactform.liquid' %}
                </div>
                <br>

                <div class="only-print">
                  <a class="url" rel="author" href="{{ content.canonicalHref }}">{{ content.shortHref }}</a>
                </div>

                <br>
                <span>{{ content.vatIdLabel }} {{ content.vatIdNumber }}</span>
                <div class="only-print"><br></div>
                <div title="{{ content.bankaccountLabel }}">
                  <div class="initially-hidden only-print">{{ content.bankaccountLabel }}</div>
                  {%- for chunk in content.bankaccountNumberChunks -%}<span class="visually-spaced">{{ chunk }}</span>{%- endfor -%}
                </div>
                <div class="paypal-link-wrapper">
                  <a
                    href="https://paypal.me/ingosteinke"
                    target="_blank"
                    rel="noopener nofollow"
                  >paypal.me/ingosteinke</a>
                </div>
                <br>
                <div>
                  <a
                    href="https://www.exali.de/siegel/Ingo-Steinke"
                    target="_blank"
                    title="{{ content.insuranceTitle }}"
                    id="insuranceLink"
                    class="no-print"
                  >
                    {{ content.insurance }}
                  </a>
                  <span class="initially-hidden only-print inline">
                    <div>{{ content.insuranceTitle }}:</div>
                    www.exali.de/siegel/Ingo-Steinke
                  </span>
                </div>
                <div class="vcard-digital-wrapper">
                  <a class="vcard-digital-qr" href="vcard/" download="ingo-steinke.vcf" title="{{ content.vcardQrTitle }}">
                    <img
                      class="vcard-digital-qr-image"
                      src="img/{{ content.vcardQrImgFilename }}"
                      alt="{{ content.vcardQrImgAlt }}"
                      loading="lazy"
                      width="500"
                      height="500">
                  </a>
                </div>
              </div>

              <ul class="row social-links no-print">
                <li>
                  <a rel="noopener nofollow" href="https://www.linkedin.com/in/ingosteinke" target="_blank" title="LinkedIn-{{ content.profile }}: Ingo Steinke">
