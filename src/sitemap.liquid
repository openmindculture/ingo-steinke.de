---
permalink: '/sitemap.xml'
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ content.canonicalHref }}</loc>
    <lastmod>{{ "now" | date: "%Y-%m-%d" }}</lastmod>
  </url>{% for page in collections.addtositemap %}
  <url>
    <loc>{{ content.canonicalHrefNoTrailingSlash }}{{ page.url | url }}</loc>
    <lastmod>{{ "now" | date: "%Y-%m-%d" }}</lastmod>
  </url>{% endfor %}
</urlset>
